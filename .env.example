# Supabase configuration
SUPABASE_URL=your_supabase_url_here                 # Format: https://[project-ref].supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key     # Format: eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_JWT_SECRET=your_jwt_secret_here            # Your Supabase JWT secret for authentication

# Google OAuth credentials
GOOGLE_CLIENT_ID=your_google_client_id_here         # Format: xxxxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret      # Format: GOCSPX-xxxxxxxx

# NextAuth configuration
NEXTAUTH_URL=http://localhost:3000                  # The base URL of your site
NEXTAUTH_SECRET=your_nextauth_secret                # Generate with: openssl rand -base64 32

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here             # Format: sk-proj-xxxxx
